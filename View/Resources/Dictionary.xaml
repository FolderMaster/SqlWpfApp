    </DataTemplate>
    <DataTemplate x:Key="ConnectionTemplate">
        <TextBlock>
            <TextBlock.Text>
                <MultiBinding StringFormat=" {0} {1} ">
                    <Binding Path="DataSource"/>
                    <Binding Path="InitialCatalog"/>
                </MultiBinding>
            </TextBlock.Text>
        </TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="AuthorizationTemplate">
        <TextBlock Text="{Binding User}"/>
    </DataTemplate>

